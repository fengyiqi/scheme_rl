<configuration>
    <domain>
        
        <nodeSize> 0.15  </nodeSize>

        
        <nodeRatio>
            <x> 2 </x>
            <y> 2 </y>
            <z> 1 </z>
        </nodeRatio>

        <boundaryConditions>
            <material>
                
                <west>Symmetry</west>
                <east>ZeroGradient</east>
                <south>Symmetry</south>
                <north>ZeroGradient</north>
                <bottom>Symmetry</bottom>
                <top>Symmetry</top>
            </material>

            <levelSet>
                
                <west>   symmetry </west>
                <east>   symmetry </east>
                <south>  symmetry </south>
                <north>  symmetry </north>
                <bottom> symmetry </bottom>
                <top>    symmetry </top>
            </levelSet>
        </boundaryConditions>

        <initialConditions>
            <material1>
	        if ((x+y) &lt; 0.15000000001)
       	 	{
                	density := 0.125;
                	velocityX := 0.0;
                	velocityY := 0.0;
                	velocityZ := 0.0;
                	pressure := 0.14;
        	}
        	else
        	{
                	density := 1.0;
                	velocityX := 0.0;
                	velocityY := 0.0;
                	velocityZ := 0.0;
                	pressure := 1.0;
        	}
            </material1>

            <levelSet1>
                phi := 1.0;
            </levelSet1>
        </initialConditions>
    </domain>

   
   
   <materials>
      <numberOfMaterials> 1   </numberOfMaterials>
      <material1>
         <equationOfState>
            <type> StiffenedGas </type>
            <gamma>               1.4   </gamma>
            <A>                   0.0   </A>
            <B>                   0.0   </B>
            <C>                   0.0   </C>
            <rho0>                0.0   </rho0>
            <specificGasConstant> 0.0   </specificGasConstant>
            <backgroundPressure> 0.0 </backgroundPressure>
	      </equationOfState>
         <properties>
            <specificHeatCapacity> 0.0 </specificHeatCapacity>
            <thermalConductivity> 0.0 </thermalConductivity>
            <shearViscosity> 0.0 </shearViscosity>
            <bulkViscosity> 0.0 </bulkViscosity>
         </properties>
      </material1>
   </materials>

    <sourceTerms>
        <gravity>
            <x> 0 </x>
            <y> 0 </y>
            <z> 0 </z>
        </gravity>
    </sourceTerms>

   <multiResolution>
       <maximumLevel> 0 </maximumLevel>

       <refinementCriterion>
           <epsilonReference>    0.01 </epsilonReference>
           
           <levelOfEpsilonReference> 0  </levelOfEpsilonReference>
       </refinementCriterion>
   </multiResolution>

   <timeControl>
      <startTime> 0.0  </startTime>
      <endTime>0.360</endTime>
      <CFLNumber> 0.6 </CFLNumber>
   </timeControl>

   <dimensionalization>
      <lengthReference>       1.0  </lengthReference>
      <velocityReference>     1.0  </velocityReference>
      <densityReference>      1.0  </densityReference>
      <temperatureReference>  1.0  </temperatureReference>
   </dimensionalization>

   <restart>
      <restore>
         <mode>Forced</mode> 
         <fileName>implosion_outflow_64_0.320/restart/restart_0.320000.h5</fileName>
      </restore>
      <snapshots>
         <type>Stamps</type> 
         <interval> 3600 </interval> 
         <intervalsToKeep> 2 </intervalsToKeep> 
         <stamps>
             <ts1>0.360</ts1>
         </stamps>
      </snapshots>
   </restart>

   <output>
      <timeNamingFactor> 1.e0 </timeNamingFactor>
      <standardOutput> 
         <type> Interval </type> 
         <interval>  0.1 </interval>
	      <stamps>
            <ts1> 0.0004 </ts1>
         </stamps>
      </standardOutput>
      <interfaceOutput> 
         <type> Off </type> 
         <interval>  1e3 </interval>
	      <stamps>
            <ts1>  0.0004 </ts1>
         </stamps>
      </interfaceOutput>
   </output>
</configuration>