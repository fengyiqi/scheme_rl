<configuration>
    <domain>
        
        <nodeSize> 0.25  </nodeSize>

        
        <nodeRatio>
            <x> 16 </x>
            <y> 16 </y>
            <z> 1 </z>
        </nodeRatio>

        <boundaryConditions>
            <material>
                
                <west>FixedValue</west>
                <valuesWest>
                    <density>8.0</density>
                    <velocityX>7.145</velocityX>
                    <velocityY>-4.125</velocityY>
                    <velocityZ>0.0</velocityZ>
                    <pressure>116.5</pressure>
                    <temperature>1.0</temperature>
                </valuesWest>
                <east>ZeroGradient</east>
                <south>Symmetry</south>
                <north>ZeroGradient</north>
                <bottom>Symmetry</bottom>
                <top>Symmetry</top>
            </material>

            <levelSet>
                
                <west>   symmetry </west>
                <east>   symmetry </east>
                <south>  symmetry </south>
                <north>  symmetry </north>
                <bottom> symmetry </bottom>
                <top>    symmetry </top>
            </levelSet>
        </boundaryConditions>

        <initialConditions>
            <material1>
	        if (y &lt; 1.732 * (x - 1.0 / 6.0))
       	 	{
                	density := 1.4;
                	velocityX := 0.0;
                	velocityY := 0.0;
                	velocityZ := 0.0;
                	pressure := 1.0;
        	}
        	else
        	{
                	density := 8.0;
                	velocityX := 7.145;
                	velocityY := -4.125;
                	velocityZ := 0.0;
                	pressure := 116.5;
        	}
            </material1>

            <levelSet1>
                phi := 1.0;
            </levelSet1>
        </initialConditions>
    </domain>

   
   
   <materials>
      <numberOfMaterials> 1   </numberOfMaterials>
      <material1>
         <equationOfState>
            <type> StiffenedGasSafe </type>
            <gamma>               1.4   </gamma>
            <A>                   0.0   </A>
            <B>                   0.0   </B>
            <C>                   0.0   </C>
            <rho0>                0.0   </rho0>
            <specificGasConstant> 0.0   </specificGasConstant>
            <backgroundPressure> 0.0 </backgroundPressure>
	      </equationOfState>
         <properties>
            <specificHeatCapacity> 0.0 </specificHeatCapacity>
            <thermalConductivity> 0.0 </thermalConductivity>
            <shearViscosity> 0.0 </shearViscosity>
            <bulkViscosity> 0.0 </bulkViscosity>
         </properties>
      </material1>
   </materials>

    <sourceTerms>
        <gravity>
            <x> 0 </x>
            <y> 0 </y>
            <z> 0 </z>
        </gravity>
    </sourceTerms>

   <multiResolution>
       <maximumLevel> 0 </maximumLevel>

       <refinementCriterion>
           <epsilonReference>    0.01 </epsilonReference>
           
           <levelOfEpsilonReference> 0  </levelOfEpsilonReference>
       </refinementCriterion>
   </multiResolution>

   <timeControl>
      <startTime> 0.0  </startTime>
      <endTime>0.2</endTime>
      <CFLNumber> 0.8 </CFLNumber>
   </timeControl>

   <dimensionalization>
      <lengthReference>       1.0  </lengthReference>
      <velocityReference>     1.0  </velocityReference>
      <densityReference>      1.0  </densityReference>
      <temperatureReference>  1.0  </temperatureReference>
   </dimensionalization>

   <restart>
      <restore>
         <mode>Off</mode> 
         <fileName>None</fileName>
      </restore>
      <snapshots>
         <type>Stamps</type> 
         <interval> 3600 </interval> 
         <intervalsToKeep> 2 </intervalsToKeep> 
         <stamps>
             <ts1>0.100</ts1>
             <ts2>0.200</ts2>
             <ts3>0.300</ts3>
             <ts4>0.400</ts4>
             <ts5>0.500</ts5>
             <ts6>0.600</ts6>
             <ts7>0.700</ts7>
             <ts8>0.800</ts8>
             <ts9>0.900</ts9>
             <ts10>1.000</ts10>
             <ts11>1.100</ts11>
             <ts12>1.200</ts12>
             <ts13>1.300</ts13>
             <ts14>1.400</ts14>
             <ts15>1.500</ts15>
             <ts16>1.600</ts16>
             <ts17>1.700</ts17>
             <ts18>1.800</ts18>
             <ts19>1.900</ts19>
             <ts20>2.000</ts20>
             <ts21>2.100</ts21>
             <ts22>2.200</ts22>
             <ts23>2.300</ts23>
             <ts24>2.400</ts24>
         </stamps>
      </snapshots>
   </restart>

   <output>
      <timeNamingFactor> 1.e0 </timeNamingFactor>
      <standardOutput> 
         <type> Interval </type> 
         <interval>  0.02 </interval>
	      <stamps>
            <ts1> 0.0004 </ts1>
         </stamps>
      </standardOutput>
      <interfaceOutput> 
         <type> Off </type> 
         <interval>  1e3 </interval>
	      <stamps>
            <ts1>  0.0004 </ts1>
         </stamps>
      </interfaceOutput>
   </output>
</configuration>
